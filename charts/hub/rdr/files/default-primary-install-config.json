{
  "apiVersion": "v1",
  "baseDomain": "cluster.example.com",
  "metadata": {
    "name": "ocp-primary"
  },
  "controlPlane": {
    "name": "master",
    "replicas": 3,
    "platform": {
      "aws": {
        "type": "m5.4xlarge"
      }
    }
  },
  "compute": [
    {
      "name": "worker",
      "replicas": 3,
      "platform": {
        "aws": {
          "type": "m5.metal"
        }
      }
    }
  ],
  "networking": {
    "clusterNetwork": [
      {
        "cidr": "10.132.0.0/14",
        "hostPrefix": 23
      }
    ],
    "machineNetwork": [
      {
        "cidr": "10.1.0.0/16"
      }
    ],
    "networkType": "OVNKubernetes",
    "serviceNetwork": [
      "172.20.0.0/16"
    ]
  },
  "platform": {
    "aws": {
      "region": "us-west-1",
      "userTags": {
        "project": "ValidatedPatterns"
      }
    }
  },
  "publish": "External",
  "sshKey": "",
  "pullSecret": ""
}
